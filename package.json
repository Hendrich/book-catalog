{
	"name": "book-catalog-app",
	"version": "1.0.0",
	"description": "A secure Book Catalog API with Node.js, Express, and PostgreSQL",
	"main": "backend/server.js",
	"scripts": {
		"start": "node backend/server.js",
		"dev": "nodemon backend/server.js",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:coverage:open": "jest --coverage && start coverage/lcov-report/index.html",
		"test:silent": "jest --silent",
		"test:verbose": "jest --verbose",
		"lint": "eslint backend/",
		"lint:fix": "eslint backend/ --fix"
	},
	"keywords": [
		"nodejs",
		"express",
		"postgresql",
		"supabase",
		"jwt",
		"rest-api",
		"book-catalog"
	],
	"author": "Hendri Christianto",
	"license": "MIT",
	"dependencies": {
		"@supabase/supabase-js": "^2.50.0",
		"bcrypt": "^6.0.0",
		"bcryptjs": "^3.0.2",
		"body-parser": "^1.20.3",
		"compression": "^1.8.1",
		"cors": "^2.8.5",
		"dotenv": "^16.5.0",
		"express": "^4.21.2",
		"express-rate-limit": "^7.5.1",
		"express-session": "^1.18.2",
		"helmet": "^7.2.0",
		"joi": "^17.13.3",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.16.0",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.17.0"
	},
	"devDependencies": {
		"eslint": "^8.55.0",
		"jest": "^29.7.0",
		"nodemon": "^3.0.2",
		"supertest": "^6.3.3"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=8.0.0"
	}
}
